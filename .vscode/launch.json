{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Causable Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-causable"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vscode-causable/out/**/*.js"
      ],
      "preLaunchTask": "build-extension",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
